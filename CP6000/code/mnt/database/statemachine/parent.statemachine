STATEMACHINEVERSION 3
Name=Parent
Link=WorkCell WorkCell

State=ST_HALT
  SETSTATE ST_IDLE
END

State=ST_TIMER
END

State=ST_IDLE
  TEST WorkCell = ST_STARTING
  OR WorkCell = ST_RESTARTING
    SETSTATE ST_RESET
  ENDTEST
END

State=ST_RUN
  TEST WorkCell = ST_IDLE
  OR WorkCell = ST_PAUSED
  OR WorkCell = ST_PAUSING
  OR WorkCell = ST_ERROR
    SETSTATE ST_IDLE
  ENDTEST
END

State=ST_RESET
  SETSTATE ST_RUN
END
