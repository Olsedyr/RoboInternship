STATEMACHINEVERSION 3
Name=ErrorCode
Link=parent Parent
linkValue=error1 errorCode WorkCell
linkValue=error2 errorCode Emergency
linkValue=error3 errorCode Robot
linkValue=error4 errorCode TripWatch
linkValue=error5 errorCode RoboError
linkValue=error6 errorCode IOError
linkValue=error7 errorCode CardBoardShuttle
linkValue=error8 errorCode ProgramB2
linkValue=error9 errorCode ServoCardBoard
linkValue=error10 errorCode CardBoardConveyor
linkValue=error11 errorCode ScissorDepalletizer2_3
linkValue=error12 errorCode PalletMagazine

State=ST_HALT
END

State=ST_TIMER
END

State=ST_IDLE
	TEST parent = ST_RESET
		SETSTATE ST_RESET
	ENDTEST
END

State=ST_RESET
	SETSTATE ST_WATCH
END

State=ST_WATCH
	TEST error1 != 0
	OR error2 != 0
	OR error3 != 0
	OR error4 != 0
	OR error5 != 0
	OR error6 != 0
	OR error7 != 0
	OR error8 != 0
	OR error9 != 0
	OR error10 != 0
	OR error11 != 0
	OR error12 != 0
		SETSTATE ST_ERROR
	ENDTEST
	TEST parent = ST_IDLE
		SETSTATE ST_IDLE
	ENDTEST
END

State=ST_ERROR
	TEST parent = ST_RESET
		SETSTATE ST_RESET
	ENDTEST
END
