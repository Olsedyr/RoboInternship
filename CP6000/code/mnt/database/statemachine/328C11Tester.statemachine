STATEMACHINEVERSION 3
Name=C11Tester
Input=Foto "Pallebane4 C11Test"
Timeout=TimeSw 300000
Value=errorCode 0
Link=parent Parent
Const=enable 0

State=ST_HALT
END

State=ST_TIMER
END

State=ST_ERROR
	SET errorCode 1010
	TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST
END

State=ST_IDLE
  TEST parent = ST_RESET
	AND enable = 1
    SETSTATE ST_RESET
  ENDTEST
END

State=ST_RESET
	SET errorCode 0
  TEST Foto = 0
    SETSTATE ST_C11
  ELSE
    SETSTATE ST_C18
  ENDTEST
END

State=ST_GOC11
  TEST TimeSw = 1
    SETSTATE ST_C11
  ENDTEST
  TEST Foto = 1
    SETSTATE ST_C18
  ENDTEST
END

State=ST_C11
  TEST Foto = 1
    TIMEOUT TimeSw ST_GOC18
    SETSTATE ST_GOC18
  ENDTEST
END

State=ST_GOC18
  TEST TimeSw = 1
    SETSTATE ST_C18
  ENDTEST
  TEST Foto = 0
    SETSTATE ST_C11
  ENDTEST
END

State=ST_C18
  TEST Foto = 0
    TIMEOUT TimeSw ST_GOC11
    SETSTATE ST_GOC11
  ENDTEST
END
