State=ST_RUNNING_C11_PEBER_1LAG
	SET newprogram 0
  TEST workcell = ST_IDLE
  OR workcell = ST_PAUSED
    TEST programidx != program
      SETSTATE ST_RESET
    ENDTEST
  ENDTEST
END

State=ST_RUNNING_C11_GRAPE_1LAG
	SET newprogram 0
  TEST workcell = ST_IDLE
  OR workcell = ST_PAUSED
