    SETSTATE PROGRAM_C11_PEBER_1LAG
  ENDTEST
  TEST programidx = 1
    SETSTATE PROGRAM_C11_GRAPE_1LAG
  ENDTEST
  TEST programidx = 2
    SETSTATE PROGRAM_C18_PEBER_2LAG
  ENDTEST
  TEST programidx = 3
    SETSTATE PROGRAM_DobbeltD9_KIWI_1LAG
  ENDTEST
END
