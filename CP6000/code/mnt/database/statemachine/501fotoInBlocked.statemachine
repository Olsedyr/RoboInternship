STATEMACHINEVERSION 2
Name=fotoInBlocked
Timeout=0,Time 1000000
Link=0,fotoIn

State=0,ST_HALT
  END

State=0,ST_TIMER
  END

State=0,ST_IDLE
  END

State=0,ST_FREE
  TEST fotoIn = ST_HIGH
    TIMEOUT Time ST_PRODUCT
    SETSTATE ST_PRODUCT
  ENDTEST
  END

State=0,ST_PRODUCT
  TEST fotoIn = ST_LOW
    SETSTATE ST_FREE
  ELSE
    TEST Time = 1
      SETSTATE ST_BLOCKED
    ENDTEST
  ENDTEST
  END

State=0,ST_BLOCKED
  TEST fotoIn = ST_LOW 
    SETSTATE ST_FREE
  ENDTEST
  END

State=0,ST_RESET
  TIMEOUT Time ST_FREE
  SETSTATE ST_FREE
END
