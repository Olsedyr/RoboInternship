STATEMACHINEVERSION 3
Include=conveyorkasseloader.statemachine
Name=KasseLoader3
Output=motor "KasseLoader 3 Motor"
Output=skubber "KasseLoader 3 Skubber"
;minihack: if Kasseloader doesn't have a stopper set it to any or empty output and set hasStopper = 0, the output will then not be set
Output=stopper "KasseLoader 1 Stopper"
Link=prev KasseLoader2
Link=skubberReceiver KasseInLinerPrevBack3
;conveyorReceiver only used if hasStopper = 1
Link=conveyorReceiver KasseLoader3
Link=foto KasseLoader3Foto
Link=parent ConveyorParent
;linkValue=loadfromleft Program
Const=hasStopper 0
;whether or not prev sends from side or back of KasseLoader
Const=sideReceive 0
Value=m1121 1126
Timeout=motorStopTime 1500000
Const=resetOnError 0
