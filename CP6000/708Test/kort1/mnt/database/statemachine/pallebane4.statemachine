STATEMACHINEVERSION 3
Name=Pallebane4
Delaystop=10000000
Input=palletInPlace "Pallebane4 FotoRev"
Input=fotoTest "Pallebane4 FotoTest"
Output=MotorFwd "Pallebane4 MotorFwd" 
Output=MotorRev "Pallebane4 MotorRev"
Output=Chain "Pallebane4 Chain"
Output=Cyl "Pallebane4 Cyl"
Link=next Pallebane5
Link=prev Pallebane3
Link=parent Parent
linkValue=nextpala Frames
Timeout=TimeoutDelay 5000000
Timeout=TimeoutForPalletRev 1000000
Timeout=TimeoutForPalletSideAlign 5000000
Timeout=TimeoutForPalletLift 2000000
Timeout=maxDelayStop 10000000
Timeout=StopDelay 250000

State=ST_HALT
  END

State=ST_TIMER
  END

State=ST_IDLE
  SET MotorFwd 0
  SET MotorRev 0
  SET Chain 0
  SET Cyl 0
  TEST parent = ST_RESET
    SETSTATE ST_RESET
  ENDTEST
END

State=ST_READY_ROBOT
	TEST nextpala = 1
		SETSTATE ST_READY
	ENDTEST
  TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST
END

State=ST_READY
  TEST next = ST_EMPTY
    SET MotorFwd 0
    SET MotorRev 1
    SET Chain 0
    SET Cyl 0
    TIMEOUT StopDelay ST_PALLET_REV
    SETSTATE ST_TIMER
  ELSE
    SET MotorRev 0
  ENDTEST
  TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST  
END

State=ST_SEND
  TEST next = ST_READY
    SETSTATE ST_EMPTY
    SET MotorFwd 0
    SET MotorRev 0
    SET Chain 0
    SET Cyl 0
  ELSE 
    SET MotorFwd 0
    SET MotorRev 0
    SET Chain 1
   SET Cyl 1
 ENDTEST
 TEST parent = ST_IDLE
 AND maxDelayStop = 1 
   SETSTATE ST_IDLE
 ENDTEST  
 END

State=ST_EMPTY
  TEST prev = ST_SEND
    SET MotorFwd 1
    SET MotorRev 0
    SET Chain 0
    SET Cyl 0
    TIMEOUT maxDelayStop ST_RECEIVE
    SETSTATE ST_RECEIVE
  ELSE 
    SET MotorFwd 0
    SET MotorRev 0
    SET Chain 0
   SET Cyl 0
  ENDTEST
  TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST  
END

State=ST_RECEIVE
 TEST palletInPlace = 1
    TIMEOUT TimeoutForPalletRev ST_PALLET_REV_TIMEOUT
    SETSTATE ST_TIMER
 ENDTEST
 TEST parent = ST_IDLE
 AND maxDelayStop = 1
   SETSTATE ST_IDLE
 ENDTEST  
 END

State=ST_PALLET_REV_TIMEOUT
  SET MotorRev 0
  SET MotorFwd 0
  SET Chain 0
  SET Cyl 0
  TEST palletInPlace = 1
  AND fotoTest = 0
		SET nextpala 0
    SETSTATE ST_READY_ROBOT
  ELSE 
    SETSTATE ST_RESET
  ENDTEST
END

State=ST_RESET
  TEST palletInPlace = 1
  AND fotoTest = 1
    SETSTATE ST_READY
  ELSE
    SET Chain 0
    SET Cyl 0
    SET MotorRev 0
    SET MotorFwd 1
    TIMEOUT TimeoutDelay ST_STOP
    SETSTATE ST_STOP
  ENDTEST
  END

State=ST_STOP
  TEST palletInPlace = 1
    TIMEOUT TimeoutForPalletRev ST_PALLET_REV_TIMEOUT
    SETSTATE ST_TIMER
  ELSE 
    TEST TimeoutDelay = 1
      SET Chain 0
      SET Cyl 0
      SET MotorFwd 0
      SET MotorRev 0
      SETSTATE ST_EMPTY
   ENDTEST	
 ENDTEST
 END

State=ST_PALLET_REV
  SET MotorFwd 0
  SET MotorRev 0
  SET Chain 0
  SET Cyl 1
  TIMEOUT TimeoutForPalletLift ST_EMPTY_SHIFT
  SETSTATE ST_TIMER
 END

State=ST_EMPTY_SHIFT
  SET MotorFwd 0
  SET MotorRev 0
  SET Chain 1
  SET Cyl 1
  TIMEOUT maxDelayStop ST_SEND
  SETSTATE ST_SEND
 END
