STATEMACHINEVERSION 2
Name=Plads3
Delaystop=5000000
Input=0,Foto "ConveyorIn Foto3"
Output=0,Hold "ConveyorIn holdKasse1"
Link=0,PladsLoad
Link=1,Plads4

State=0,ST_TIMER
END

State=0,ST_HALT
END

State=0,ST_IDLE
  SET Hold 0
END

State=1,ST_READY
  TEST Foto = 0
    SET Hold 0
    SETSTATE ST_EMPTY
  ELSE
    TEST Plads4 = ST_EMPTY
      SET Hold 0
      SETSTATE ST_SEND
    ELSE
      TEST Plads4 = ST_IDLE
        SETSTATE ST_IDLE   
      ENDTEST
    ENDTEST
  ENDTEST
END

State=2,ST_EMPTY
  TEST Foto = 1
    SET Hold 1
    SETSTATE ST_READY
  ELSE
    TEST Plads4 = ST_IDLE
      SETSTATE ST_IDLE   
    ENDTEST
  ENDTEST
END

State=3,ST_SEND
  TEST Plads4 = ST_READY
    TEST Foto = 0
      SET Hold 0
      SETSTATE ST_EMPTY
    ELSE
      SET Hold 1
      SETSTATE ST_READY
    ENDTEST
  ELSE
    TEST Plads4 = ST_IDLE
      SETSTATE ST_IDLE   
    ENDTEST
  ENDTEST	
END

State=4,ST_RESET
  TEST Foto = 0
    SET Hold 0    
    SETSTATE ST_EMPTY
  ELSE
    SET Hold 1
    SETSTATE ST_READY
  ENDTEST
END
