State=ST_PALLETB1_HOME
  TEST palletB1_home.state = ST_EXECUTING
		SET delaystop 0
  ELSE
    TEST palletB1_home.state = ST_FINISHED
			TEST cBItem >= wBItem
				SET cBItem sBItem
				CALC cBStackleft = sBItem / 2
				CALC cBStackright = sBItem / 2
				SET nextpalb 1
			ENDTEST
			SET loaded 0
      SETSTATE ST_HOME
    ELSE
      TEST palletB1_home.state = ST_ERROR
        TEST parent = ST_IDLE
          SETSTATE ST_IDLE
        ENDTEST 
