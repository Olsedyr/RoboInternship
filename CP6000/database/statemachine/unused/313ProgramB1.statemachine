;* ST_PALLETB0_CONVEYORB
;*******************************************
State=ST_PALLETB0_CONVEYORB
  TEST palletB0_conveyorB.state = ST_EXECUTING
		;    TEST axis_t > 900000
		;      SET griber 1
		;    ENDTEST
  ELSE
    TEST palletB0_conveyorB.state = ST_FINISHED
