State=ST_PALLETB1_CONVEYORB
  TEST palletB1_conveyorB.state = ST_EXECUTING
		;    TEST axis_t > 900000
		;      SET griber 1
		;    ENDTEST
  ELSE
    TEST palletB1_conveyorB.state = ST_FINISHED
