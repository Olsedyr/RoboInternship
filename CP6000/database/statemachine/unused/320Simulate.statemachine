STATEMACHINEVERSION 3
Name=Simulate
Link=Pos3_5
Link=Pos3_4
Link=Pos4_4
Link=Pos4_8
Link=Pos4_9
Link=Emergency
linkValue=simulatePos3_5 simulate Pos3_5
linkValue=simulatePos3_4 simulate Pos3_4
linkValue=simulatePos4_4 simulate Pos4_4
linkValue=simulatePos4_8 simulate Pos4_8
linkValue=simulatePos4_9 simulate Pos4_9
linkValue=simulateEmergency simulate Emergency
Value=simulate 2

State=ST_HALT
END

State=ST_TIMER
END

State=ST_IDLE
  TEST simulate = 0
    SET simulatePos3_5 0
    SET simulatePos3_4 0
    SET simulatePos4_4 0
    SET simulatePos4_8 0
    SET simulatePos4_9 0
    SET simulateEmergency 0
  ELSE
    TEST simulate = 1
      SET simulatePos3_5 1
      SET simulatePos3_4 1
      SET simulatePos4_4 1
      SET simulatePos4_8 1
      SET simulatePos4_9 1
      SET simulateEmergency 1
    ELSE
;=2 enable individual settings of simulate
    SET simulateEmergency 1
    ENDTEST
  ENDTEST
END

