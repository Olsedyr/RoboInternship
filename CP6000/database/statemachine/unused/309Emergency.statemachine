STATEMACHINEVERSION 2
Name=Emergency
Value=0,errorCode 0
Value=1,emergency 1

State=0,ST_TIMER
END

State=0,ST_HALT
  TEST emergency = 1
		SETSTATE ST_POWEROFF
  ELSE
    SETSTATE ST_POWERON
  ENDTEST
END

State=0,ST_IDLE
  TEST emergency = 1
		SETSTATE ST_POWEROFF
  ELSE
    SETSTATE ST_POWERON
  ENDTEST
END

State=0,ST_POWEROFF
	SET errorCode 101
	TEST emergency = 0
		SETSTATE ST_POWERON
	ENDTEST
END

State=0,ST_POWERON
	SET errorCode 0
	TEST emergency = 1
		SETSTATE ST_POWEROFF
	ENDTEST
END

State=5,ST_RESET
END
