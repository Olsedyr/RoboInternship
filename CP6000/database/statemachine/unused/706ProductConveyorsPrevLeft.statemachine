STATEMACHINEVERSION 3
Name=ProductConveyorsPrevLeft3
Link=parent Parent
Link=next ProductConveyors3
Const=useDontSend 0
linkValue=dontSend gotLeft ProductCount3

State=ST_TIMER
END

State=ST_HALT
END

State=ST_IDLE
	TEST parent = ST_RESET
		SETSTATE ST_RESET
	ENDTEST
END

State=ST_RESET
  SETSTATE ST_STOP
END

State=ST_RUN
  TEST next != ST_RUN
    SETSTATE ST_STOP
  ENDTEST
  TEST useDontSend = 1
  AND dontSend = 1
    SETSTATE ST_STOP
  ENDTEST
  TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST
END

State=ST_STOP
  TEST useDontSend = 1
    TEST dontSend = 0
    AND next = ST_RUN
      SETSTATE ST_RUN
    ENDTEST
  ELSE
    TEST next = ST_RUN
      SETSTATE ST_RUN
    ENDTEST
  ENDTEST
	TEST parent = ST_IDLE
    SETSTATE ST_IDLE
  ENDTEST
END
