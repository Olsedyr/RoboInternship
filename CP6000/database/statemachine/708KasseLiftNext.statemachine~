STATEMACHINEVERSION 3
Name=KasseLiftNext
Link=prev KasseLift
Link=next ConveyorOut
Value=recv1 1

State=ST_TIMER
END

State=ST_HALT
END

State=ST_IDLE
	SETSTATE ST_RESET
END

State=ST_RESET
  SETSTATE ST_EMPTY
END

State=ST_EMPTY
  TEST prev = ST_SEND
	AND next = ST_MOVE
  OR prev = ST_SEND
	AND next = ST_FILL_BUFFER_SPACE2
  OR prev = ST_SEND
	AND next = ST_FILL_BUFFER_SPACE1
    SETSTATE ST_RECEIVE
  ENDTEST
END

State=ST_RECEIVE
  TEST prev != ST_SEND
    SETSTATE ST_EMPTY
  ENDTEST
END
