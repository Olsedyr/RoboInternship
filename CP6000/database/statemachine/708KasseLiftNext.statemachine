STATEMACHINEVERSION 3
Name=KasseLiftNext
Link=prev KasseLift
Value=recv1 1

State=ST_TIMER
END

State=ST_HALT
END

State=ST_IDLE
	SETSTATE ST_RESET
END

State=ST_RESET
  SETSTATE ST_EMPTY
END

State=ST_EMPTY
  TEST prev = ST_SEND
    SETSTATE ST_RECEIVE
  ENDTEST
END

State=ST_RECEIVE
  TEST prev != ST_SEND
    SETSTATE ST_EMPTY
  ENDTEST
END
