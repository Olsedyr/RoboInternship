State=PROGRAM_TEST
	SET usersection 0
	SET newprogram 1
	SET loadfromleft 1
	SET loadfromright 0
	SET productsfromleft 0
	SET productsfromright 1
	SET program 6
  SET manuelt 0
	SET usersection 1 ; user editable below
  SETITEM item itemC14
	SETCPPATTERN cppattern cppUser2
	SET timeMotorInGoDown 0
	SET timeKlapDown 300000
	SET noShakeOnLast 0
	SET waitInR3 0
	SET vibrateTime 0
	SETSTATE ST_RUNNING_TEST
END

State=ST_RUNNING_STD1
	SET newprogram 0
  TEST workcell = ST_IDLE
  OR workcell = ST_PAUSED
    TEST programidx != program
      SETSTATE ST_RESET
    ENDTEST
  ENDTEST
END

State=ST_RUNNING_FERVAA_10
	SET newprogram 0
  TEST workcell = ST_IDLE
  OR workcell = ST_PAUSED
    TEST programidx != program
      SETSTATE ST_RESET
    ENDTEST
  ENDTEST
END

