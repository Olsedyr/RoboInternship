      SETSTATE ST_RESET
    ENDTEST
  ENDTEST
END

State=ST_TIMER
END

State=ST_HALT
END

State=ST_IDLE
	SETSTATE ST_RESET
END

State=ST_RESET
  TEST programidx = 0
    SETSTATE PROGRAM_STD1
  ENDTEST
  TEST programidx = 1
    SETSTATE PROGRAM_FERVAA_10
  ENDTEST
  TEST programidx = 2
    SETSTATE PROGRAM_FERVATEST_10
  ENDTEST
	TEST programidx = 3
    SETSTATE PROGRAM_HARNDRUP
  ENDTEST
  TEST programidx = 4
    SETSTATE PROGRAM_MANUAL
  ENDTEST
  TEST programidx = 5
    SETSTATE PROGRAM_C14
  ENDTEST
  TEST programidx = 6
    SETSTATE PROGRAM_TEST
  ENDTEST
END
