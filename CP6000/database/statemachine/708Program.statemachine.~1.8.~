STATEMACHINEVERSION 3
Name=Program
Link=workcell WorkCell
Value=usersection 0
Value=programidx 1
Value=loadfromleft 0
Value=loadfromright 0
Value=productsfromleft 0
Value=productsfromright 0
Value=newprogram 1
Value=timeMotorInGoDown 0
Value=timeKlapDown 0
Value=noShakeOnLast 0
Value=waitInR3 0
Value=vibrateTime 8000000
Value=manuelt 0
Value=kasseStopType 1
Value=secondProdConvOn 1
;programmer
Value=program 0
Item=item Netto
Item=itemNetto Netto
Item=itemC18 C18
Item=itemC14 C14
Item=itemIFCO18 IFCO18
Item=itemFervaA FervaA
Item=itemFervaTest FervaTest
CPPattern=cppattern Test
CPPattern=cppTest Test
CPPattern=cppUser1 User1
CPPattern=cppUser2 User2
CPPattern=cppFerva Ferva
CPPattern=cppHarndrup Harndrup

State=PROGRAM_STD1
	SET usersection 0
	SET newprogram 1
	SET loadfromleft 1
	SET loadfromright 0
	SET productsfromleft 0
	SET productsfromright 1
	SET program 0
  SET manuelt 0
	SET usersection 1 ; user editable below
  SETITEM item itemFervaA
  SETCPPATTERN cppattern cppFerva
	SET timeMotorInGoDown 0
  	SET timeKlapDown 500000
	SET noShakeOnLast 0
	SET waitInR3 1
	SET vibrateTime 0
	SETSTATE ST_RUNNING_STD1
END

State=PROGRAM_FERVAA_10
	SET usersection 0
	SET newprogram 1
	SET loadfromleft 1
	SET loadfromright 0
	SET productsfromleft 0
	SET productsfromright 1
	SET program 1
  SET manuelt 0
	SET usersection 1 ; user editable below
  SETITEM item itemFervaA
  SETCPPATTERN cppattern cppFerva
	SET timeMotorInGoDown 0
	SET timeKlapDown 300000
	SET noShakeOnLast 0
	SET waitInR3 0
	SET vibrateTime 0
	SETSTATE ST_RUNNING_FERVAA_10
END

State=PROGRAM_FERVATEST_10
	SET usersection 0
	SET newprogram 1
	SET loadfromleft 1
	SET loadfromright 0
	SET productsfromleft 0
	SET productsfromright 1
	SET program 2
  SET manuelt 0
	SET usersection 1 ; user editable below
  SETITEM item itemFervaTest
  SETCPPATTERN cppattern cppFerva
	SET timeMotorInGoDown 0
	SET timeKlapDown 300000
	SET noShakeOnLast 0
	SET waitInR3 0
	SET vibrateTime 0
	SETSTATE ST_RUNNING_FERVATEST_10
END

